# Local development environment variables
# Copy this to .env for local development

# Eufy Credentials
EUFY_USERNAME=your_email@example.com
EUFY_PASSWORD=your_password

# Workato Integration
WORKATO_WEBHOOK_URL=https://webhooks.workato.com/webhooks/rest/YOUR_ID/eufy

# Application Settings
LOG_LEVEL=DEBUG
PYTHONUNBUFFERED=1

# Local WebSocket (for docker-compose)
EUFY_WS_URL=ws://127.0.0.1:3000/ws

# Public URL (for local testing)
RENDER_EXTERNAL_URL=http://localhost:10000